# the names of the formatted checkpoint files
ground_fch  'h2_g.fch'
excited_fch 'h2_e.fch'
anion_fch   'h2_a.fch'
cation_fch  'h2_c.fch'
# information for calculating the effective HR factors
lower_bound 0.d0
upper_bound 5.d4
# method for ground/cation/anion calculations
method      'B3LYP/cc-pVTZ'
# method for excited calculation
emethod     'TD=(nstates=1,root=1) B3LYP/cc-pVTZ'
# name of input files generated
task        h2
# xyz file containing coordinates to be optimized
xyz_file    h2.xyz
